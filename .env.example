# Backend API base URL for the app
# - Web / iOS simulator: http://localhost:4000
# - Android emulator: http://10.0.2.2:4000
# - Real device: http://<your-lan-ip>:4000
EXPO_PUBLIC_API_URL=http://localhost:4000

# Web demo mode (no backend required). Useful for GitHub Pages.
EXPO_PUBLIC_DEMO_MODE=false
# Example environment variables (rename to .env locally and fill with real values)
# Do NOT commit your real .env to git. The repository .gitignore excludes /.env and /.env.local

# Test user credentials (used by the local fake API)
TEST_USER_EMAIL=teste@banana.com
TEST_USER_PASSWORD=123456

# Add other keys here, e.g.:
# EXPO_PUBLIC_MAP_KEY=your_public_key_here
